<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/static/css/signin.css" />
    <%- include('../common/include.ejs') %>
    <script type="text/javascript">
      window.history.forward();
      function noBack(){window.history.forward();}
     </script>
  </head>
  <body onload="noBack();" onpageshow="if(event.persisted) noBack();" onunload="">
    <%- include('../common/nav2.ejs') %>
    <body>
      <div class="imgArea1">
        <img src="/static/img/signin_img1.png" alt="loginImg" class="loginImg1" />
      </div>
      <div class="imgArea1">
        <img src="/static/img/signin_img3.png" alt="loginImg" class="loginImg3" />
      </div>
      <div class="loginArea">
        <img src="/static/img/signin_logo.PNG" alt="logo" class="logoImg" />
        <div class="loginForm">
          <form name="signinForm" id="signinForm">
            <div>
              <input
                name="userID"
                type="text"
                maxlength="20"
                placeholder="아이디"
              />
            </div>
            <div>
              <input
                name="userPW"
                type="password"
                maxlength="20"
                placeholder="비밀번호"
              />
            </div>
            <button type="button" class="loginBtn" onclick="login();">로그인</button>
          </form>
          <br /><br />
          <a href="/user/signup" class="signupLink"
            ><span class="noAccount">계정이 없으신가요?</span> <strong>가입하기</strong></a
          >
        </div>
      </div>
      <div class="imgArea2">
        <img src="/static/img/signin_img2.png" alt="loginImg" class="loginImg2" />
      </div>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="/static/js/signin.js"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  </body>
</html>
